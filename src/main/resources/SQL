CREATE TABLE IF NOT EXISTS ACCOUNT(
USER_ID VARCHAR(100) NOT NULL COMMENT '用户id',
CARD_NO VARCHAR(100) NOT NULL COMMENT '账号',
MONEY INT NOT NULL COMMENT '金额'
)ENGINE=INNODB DEFAULT CHARSET=UTF8;

DELETE FROM ACCOUNT;
INSERT INTO ACCOUNT VALUES('1000001','200001',1200);
INSERT INTO ACCOUNT VALUES('1000002','200002',1100);


select * from ACCOUNT ORDER BY CARD_NO;